{
  "name": "handy-collapse",
  "version": "1.0.4",
  "description": "A pure Javascript module for accordion/collapse UI without JQuery",
  "main": "./dist/handy-collapse.cjs",
  "module": "./dist/handy-collapse.mjs",
  "types": "./dist/handy-collapse.d.ts",
  "exports": {
    ".": {
      "require": "./dist/handy-collapse.cjs",
      "import": "./dist/handy-collapse.mjs"
    }
  },
  "author": "R Sakai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sk-rt/handy-collapse.git"
  },
  "keywords": [
    "js",
    "vanillajs",
    "micro",
    "collapse",
    "accordion",
    "ui",
    "slidedown",
    "dropdown",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "build:sample": "vite build --config vite.config.sample.ts",
    "prepare": "yarn build",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "dts-bundle-generator": "^7.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.6"
  },
  "dependencies": {}
}
